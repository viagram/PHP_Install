<config>
<cmd name='php7100' file='%{config_dir}/bin/php-cgi' listen='local' lock='0' type='mp' proto='fastcgi'>
<pre_event src_file='%{config_dir}/etc/php.ini' dst_file='/tmp/php-${name}.ini'/>
<env PHP_FCGI_MAX_REQUESTS='0' PHPRC='/tmp/php-${name}.ini'/>
</cmd>
<vhs  index='index.php'>
<map file_ext='php' extend='cmd:php7100' allow_method='*'/>
</vhs>
<vh_templete name='php:php7100' templete='html' index='index.php'>
<map file_ext='php' extend='cmd:php7100' allow_method='*'/>
</vh_templete>
</config>
